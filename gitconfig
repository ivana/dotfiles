[alias]
  st = status
  br = branch
  co = checkout
  ci = commit
  ca = commit -a
  amend = commit --amend -C HEAD
  amendm = commit --amend
  ri = rebase -i
  rc = rebase --continue
  ra = rebase --abort
  cpk = cherry-pick
  lg = log --graph --pretty=format:'%C(auto)%h %cD %Cred%cN %Cgreen%s'
  stashdiff = stash show -p
